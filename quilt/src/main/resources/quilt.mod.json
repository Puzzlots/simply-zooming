{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {
        "Spicylemon": "Developer",
        "CrabKing": "Developer"
      },


      "contact": {
        "homepage": "",
        "issues": "https://github.com/Spicylemon2623/simply-zooming/issues",
        "sources": "https://github.com/Spicylemon2623/simply-zooming/"
      },

      "icon": "assets/${mod_id}/icon.png"
    },

    "entrypoints": {
      "init": "io.github.spicylemon2623.SimplyZooming.SimplyZooming"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": ">=${cosmic_quilt_version}"
      },
      {
        "id": "cosmicreach",
        "versions": ">=${cosmic_reach_version}"
      }
    ]
  },

  "access_widener": "${mod_id}.accesswidener",
  "mixin": "${mod_id}.mixins.json"
}
