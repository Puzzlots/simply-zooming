{
  "formatVersion": 3,
  "display-name": "${mod_name}",
  "id": "${mod_id}",
  "version": "${mod_version}",
  "description": "${mod_desc}",
  "authors": [
    "Spicylemon",
    "CrabKing"
  ],
  "can-load-on": {
    "client": true,
    "server": true
  },
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "client_init": [
      "io.github.spicylemon2623.SimplyZooming.SimplyZoomingClient"
    ],
    "init": [
      "io.github.spicylemon2623.SimplyZooming.SimplyZooming"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "cosmic-reach": ">=${cosmic_reach_minimum}",
    "puzzle-loader-cosmic": ">=${puzzle_loader_cosmic}",
    "puzzle-loader-core": ">=${puzzle_loader_core}"
  },
  "accessManipulator": "${mod_id}.manipulator"
}
